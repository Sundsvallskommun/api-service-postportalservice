{
	"request": {
		"method": "POST",
		"urlPathPattern": "/api-messaging/2281/snail-mail",
		"queryParameters": {
			"batchId": {
				"matches": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$"
			}
		},
		"bodyPatterns": [
			{
				"equalToJson": {
					"party": {
						"partyId": "7a1234bc-56de-7890-1234-56789abcdef0",
						"externalReferences": []
					},
					"address": {
						"firstName": "John",
						"lastName": "Doe",
						"address": "Testgatan 12",
						"zipCode": "54321",
						"city": "Testholm",
						"country": "Sweden"
					},
					"department": "Testkontoret",
					"attachments": [
						{
							"filename": "test.pdf",
							"contentType": "application/pdf",
							"content": "VGVzdGluZyEgOik="
						}
					],
					"folderName": "Sundsvall"
				},
				"ignoreExtraElements": true
			}
		],
		"headers": {
			"Authorization": {
				"equalTo": "Bearer MTQ0NjJkZmQ5OTM2NDE1ZTZjNGZmZjI3"
			},
			"x-origin": {
				"equalTo": "PostPortalService"
			},
			"X-Sent-By": {
				"equalTo": "TestUser; type=adAccount"
			},
			"Content-Type": {
				"equalTo": "application/json"
			}
		}
	},
	"response": {
		"status": 201,
		"headers": {
			"Content-Type": "application/json"
		},
		"bodyFileName": "test03_successfully_sendSnailMailAndDigitalMail/responses/messaging-send-snail-mail.json"
	}
}
