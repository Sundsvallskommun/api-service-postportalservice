{
	"request": {
		"method": "POST",
		"urlPathPattern": "/api-messaging/2281/snail-mail",
		"queryParameters": {
			"batchId": {
				"matches": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$"
			}
		},
		"bodyPatterns": [
			{
				"equalToJson": {
					"party": {
						"externalReferences": []
					},
					"address": {
						"firstName": "John",
						"lastName": "Doe",
						"address": "Test Street 1337",
						"zipCode": "67890",
						"city": "Testborg",
						"country": "Sweden"
					},
					"department": "Testkontoret",
					"attachments": [
						{
							"filename": "test.pdf",
							"contentType": "application/pdf",
							"content": "VGVzdGluZyEgOik="
						}
					],
					"folderName": "Sundsvall"
				},
				"ignoreExtraElements": true
			}
		],
		"headers": {
			"Authorization": {
				"equalTo": "Bearer MTQ0NjJkZmQ5OTM2NDE1ZTZjNGZmZjI3"
			},
			"x-origin": {
				"equalTo": "PostPortalService"
			}
		}
	},
	"response": {
		"status": 201,
		"headers": {
			"Content-Type": "application/json"
		},
		"bodyFileName": "test04_successfully_snailMailToAddress/responses/messaging-send-snail-mail.json"
	}
}
